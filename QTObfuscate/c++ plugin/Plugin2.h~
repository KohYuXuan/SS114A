#include "ReverseAlgorithm.h"
#include "Plugin.h"


class Plugin2 : public Plugin {

private:

        ReverseAlgorithm m_algorithm;
        map<string,string> m_mapping;

public:
	// save user choosen path into target list
	void setUpFile();

	// read MAP files which contains list of random strings and thier original names
	void setUpMapping();

	// carray out the algorithm
	virtual void algorithm();
};


extern "C" Plugin* create() {
    return new Plugin2;
}

extern "C" void destroy(Plugin* p) {
    delete p;
}
